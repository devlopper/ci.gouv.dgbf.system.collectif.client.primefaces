<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<!-- Expenditure -->
		
		<navigation-case>
			<from-outcome>expenditureListView</from-outcome>
			<to-view-id>/private/expenditure/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>expenditureCreateView</from-outcome>
			<to-view-id>/private/expenditure/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>expenditureEditView</from-outcome>
			<to-view-id>/private/expenditure/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>expenditureAdjustView</from-outcome>
			<to-view-id>/private/expenditure/adjust/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>expenditureLoadView</from-outcome>
			<to-view-id>/private/expenditure/load/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Resource -->
		
		<navigation-case>
			<from-outcome>resourceListView</from-outcome>
			<to-view-id>/private/resource/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>resourceAdjustView</from-outcome>
			<to-view-id>/private/resource/adjust/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Regulatory Act -->
		
		<navigation-case>
			<from-outcome>regulatoryActListView</from-outcome>
			<to-view-id>/private/regulatoryact/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>regulatoryActLegislativeActVersionEditIncludedView</from-outcome>
			<to-view-id>/private/regulatoryact/legislativeact/version/editincluded.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Generated Act -->
		
		<navigation-case>
			<from-outcome>generatedActListView</from-outcome>
			<to-view-id>/private/generatedact/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Legislative Act -->
		
		<navigation-case>
			<from-outcome>legislativeActListView</from-outcome>
			<to-view-id>/private/legislativeact/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>legislativeActCreateView</from-outcome>
			<to-view-id>/private/legislativeact/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>legislativeActReadView</from-outcome>
			<to-view-id>/private/legislativeact/read/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Legislative Act Version -->
		
		<navigation-case>
			<from-outcome>legislativeActVersionListView</from-outcome>
			<to-view-id>/private/legislativeact/version/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>legislativeActVersionCreateView</from-outcome>
			<to-view-id>/private/legislativeact/version/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>legislativeActVersionReadView</from-outcome>
			<to-view-id>/private/legislativeact/version/read/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>